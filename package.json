{
  "name": "wearesho-site",
  "version": "1.0.0",
  "description": "Website for SHO Studio",
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json  NODE_ENV=test ./node_modules/.bin/nyc mocha -r jsdom-global/register -r ts-node/register -r source-map-support/register tests/unit/bootstrap.ts tests/unit/**-specs.tsx tests/unit/**-specs.ts",
    "e2e": "tsc --project tsconfig.codecept.json && codeceptjs run --steps",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "build:preview": "webpack",
    "build:docker": "docker-compose build",
    "start": "cross-env webpack-dev-server",
    "selenium": "selenium-standalone install && selenium-standalone start",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "./node_modules/.bin/tslint --project . --exclude ./node_modules/**/* --exclude ./web/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wearesho-team/wearesho-site.git"
  },
  "keywords": [
    "website",
    "react",
    "typescript"
  ],
  "author": "Alexander Letnikow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wearesho-team/wearesho-site/issues"
  },
  "homepage": "https://github.com/Wearesho-team/wearesho-site#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/enzyme": "^2.8.9",
    "@types/history": "^4.6.0",
    "@types/mocha": "^2.2.44",
    "@types/prop-types": "^15.5.1",
    "@types/react-modal": "2.2.1",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-scroll": "^1.5.1",
    "@types/sinon": "^2.3.3",
    "chai": "^4.1.2",
    "codeceptjs-webdriverio": "^1.1.0",
    "codecov": "^2.3.0",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^11.1.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.0.0",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "selenium-standalone": "^6.6.0",
    "sinon": "^3.2.0",
    "ts-node": "^3.3.0",
    "tslint": "5.7.0",
    "tslint-react": "3.2.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "@types/node": "^8.0.47",
    "@types/react": "^16.0.19",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.2",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "breakpoint-sass": "^2.7.1",
    "class-validator": "^0.7.2",
    "clean-webpack-plugin": "^0.1.16",
    "compass-mixins": "^0.12.10",
    "copy-webpack-plugin": "^4.0.1",
    "counterpart": "^0.18.3",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-break": "^1.3.0",
    "react-context-form": "^1.6.5",
    "react-dom": "^16.0.0",
    "react-input-mask": "^1.0.7",
    "react-modal": "2.2.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.1.2",
    "react-scroll": "^1.5.4",
    "react-transition-group": "^2.2.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "susy": "^2.2.12",
    "typescript": "^2.4.2",
    "webpack": "^3.5.1",
    "webpack-plugin-critical": "^1.0.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude:": [
      "**/*.d.ts",
      "webpack.config.js",
      "codecept.conf.js",
      "tests"
    ],
    "reporter": [
      "clover",
      "text-summary"
    ],
    "report-dir": "./tests/output"
  },
  "pre-commit": [
    "lint"
  ]
}
