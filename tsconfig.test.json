{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es2016",
    "sourceMap": true,
    "removeComments": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "paths": {
      "*": [
        "*"
      ]
    }
  },
  "exclude": [
    "./node_modules/**/*"
  ]
}